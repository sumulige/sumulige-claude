  "author": "sumulige",
  "license": "MIT",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sumulige/sumulige-claude.git"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "patchright": "^1.57.0"
  },
  "devDependencies": {
    "jest": "^30.2.0",
    "mock-fs": "^5.5.0",
    "prettier": "^3.7.4",
    "sinon": "^21.0.1",
    "standard-version": "^9.5.0",
    "yaml": "^2.8.2"
